{
  "manifest_version": 2,
  "name": "AutoLogin",
  "version": "0.1",
  "description": "Automatic login for the St. Mark's LMS",
  "content_scripts": [
      {
        "matches": [
          "https://smtexas.myschoolapp.com/app/"
        ],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
  "icons": { "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {}


}

  